 {
    "name": "Instalytics",
    "version": "1.0",
    "description": "Instgram Analytics Extended",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": [
      "storage",
      "unlimitedStorage",
      "tabs",
      "http://*/",
      "https://*/"
    ],
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "popup/popup.html"
    }
  }
